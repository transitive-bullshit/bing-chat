{
  "name": "bing-chat",
  "version": "0.2.3",
  "description": "Node.js client for the unofficial Bing Chat API.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/bing-chat",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del build",
    "prebuild": "run-s clean",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "docs": "typedoc",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "run-p test:*",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^18.15.3",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "del-cli": "^5.0.0",
    "dotenv-safe": "^8.2.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.2",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "tsx": "^3.12.5",
    "typedoc": "^0.23.26",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "openai",
    "bing",
    "chat",
    "search",
    "sydney",
    "gpt",
    "gpt-3",
    "gpt3",
    "gpt4",
    "chatbot",
    "machine learning",
    "conversation",
    "conversational",
    "ai",
    "ml",
    "bot"
  ]
}
